<Window x:Class="DougKlassen.Revit.Perfect.Interface.SelectElementsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DougKlassen.Revit.Perfect.Interface"
        mc:Ignorable="d"
        Title="Select Elements" Height="450" Width="400">
    <Grid>
        <ListBox x:Name="elementsListBox" Margin="30,30,30,65"/>
        <Button x:Name="cancelButton" Content="Cancel" Margin="0,0,30,22" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom"/>
        <Button x:Name="okButton" Content="OK" Margin="0,0,129,22" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" />

    </Grid>
</Window>
