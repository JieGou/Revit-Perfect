<Window x:Class="DougKlassen.Revit.Perfect.Commands.PurgeLinePatternsCommand.PurgeLinePatternsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Perfect - Purge Elements" Height="300" Width="500">
    <DockPanel LastChildFill="True">
        <Canvas Height="60" DockPanel.Dock="Top">
            <TextBlock TextWrapping="Wrap" Text="Purge Elements Matching Regular Expression:" Margin="20"/>
        </Canvas>
        <Canvas Height="40" DockPanel.Dock="Bottom">
            <Button Content="Cancel" Width="75" Canvas.Right="20" Canvas.Bottom="10"/>
            <Button Content="OK" Canvas.Right="110" Canvas.Bottom="10" Width="75"/>
        </Canvas>
        <ListBox x:Name="MatchingElementsListBox" DockPanel.Dock="Right" Width="200" Margin="10,20,10,20"/>
        <StackPanel>
            <TextBox Height="23" TextWrapping="Wrap" Text="TextBox" Margin="20"/>
        </StackPanel>
    </DockPanel>
</Window>
