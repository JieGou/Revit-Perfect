<Window x:Class="DougKlassen.Revit.Perfect.Commands.FilterCalloutsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DougKlassen.Revit.Perfect.Commands"
        mc:Ignorable="d"
        Title="Filter View Callouts" Height="450" Width="350" ResizeMode="NoResize">
    <DockPanel>
        <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right" Margin="10">
            <Button x:Name="OkButton" Content="OK" Height="20" Margin="5,1,5,1" Padding="5,2,5,3" Click="OkButton_Click"/>
            <Button x:Name="CancelButton" Content="Cancel" Height="20" Margin="5,1,5,1" Padding="5,2,5,3" Click="CancelButton_Click"/>
        </StackPanel>
    </DockPanel>
</Window>
